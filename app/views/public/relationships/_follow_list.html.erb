<div class ="container">
  <table class="follow_list">
    <tbody>
      <% users.each do |user| %>
        <tr>
          <td>
            <div class="user_profile_image">
              <%= link_to user_path(user) do %>
                <%= attachment_image_tag user, :profile_image, size: "50x50", fallback: "default_profile.jpg" %>
              <% end %>
            </div>
          </td>
          <td>
            <%= link_to user.last_name + user.first_name, user_path(user) %>
          </td>
       </tr>
      <% end %>
    </tbody>
  </table>
</div>