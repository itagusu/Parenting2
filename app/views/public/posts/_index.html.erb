
  <% posts.each do |post| %>
    <%= link_to post_path(post) do %>
      <%= attachment_image_tag(post.user, :profile_image, size: "70x70", fallback: "default_profile.jpeg") %>
    <% end %>

    <%= link_to post.user.last_name + post.user.first_name, post_path(post) %>
    <%= post.body %><br><br>
  <% end %>